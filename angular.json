{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "d88c7cb2-be43-4c88-99c0-bff5fdb3be6b"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "aml-software": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/aml-software",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/app/scss/main.scss",
              "./node_modules/owl.carousel/dist/assets/owl.carousel.min.css",
              "./node_modules/owl.carousel/dist/assets/owl.theme.default.min.css",
              "./node_modules/hover.css/css/hover.css",
              "./node_modules/aos/dist/aos.css",
              "./node_modules/@glidejs/glide/src/assets/sass/glide.core.scss",
              "./node_modules/@glidejs/glide/src/assets/sass/glide.theme.scss",
              "./node_modules/swiper/swiper.scss"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/owl.carousel/dist/owl.carousel.min.js",
              "./node_modules/aos/dist/aos.js",
              "./node_modules/swiper/swiper-bundle.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "aml-software:build:production"
            },
            "development": {
              "browserTarget": "aml-software:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "aml-software:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/app/scss/main.scss",
              "./node_modules/owl.carousel/dist/assets/owl.carousel.min.css",
              "./node_modules/owl.carousel/dist/assets/owl.theme.default.min.css",
              "./node_modules/hover.css/css/hover.css",
              "./node_modules/aos/dist/aos.css",
              "./node_modules/@glidejs/glide/src/assets/sass/glide.core.scss",
              "./node_modules/@glidejs/glide/src/assets/sass/glide.theme.scss",
              "./node_modules/swiper/swiper.scss"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/owl.carousel/dist/owl.carousel.min.js",
              "./node_modules/aos/dist/aos.js",
              "./node_modules/swiper/swiper-bundle.js"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "aml-software"
}